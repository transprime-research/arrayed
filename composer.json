{
  "name": "transprime-research/arrayed",
  "description": "PHP Array(ed) in object oriented way wrapping PHP arrays in a consistent manner.",
  "license": "MIT",
  "keywords": [
    "php",
    "Arrayed",
    "Arrays",
    "Array",
    "Helpers",
    "Laravel"
  ],
  "homepage": "https://transprime-research.github.io/arrayed/",
  "support": {
    "issues": "https://github.com/transprime-research/arrayed/issues",
    "source": "https://github.com/transprime-research/arrayed/issues"
  },
  "authors": [
    {
      "name": "Oluwatobi Samuel Omisakin",
      "email": "omisakin.oluwatobi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "transprime-research/piper": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "squizlabs/php_codesniffer": "3.*",
    "illuminate/support": ">=5.5"
  },
  "autoload": {
    "psr-4": {
      "Transprime\\Arrayed\\": "src"
    },
    "files": [
      "Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Transprime\\Arrayed\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Transprime\\Arrayed\\Providers\\ArrayedServiceProvider"
      ]
    }
  }
}
